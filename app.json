{
  "expo": {
    "name": "Gikundiro",
    "slug": "gikundiro",
    "version": "1.0.0",
    "scheme": "gikundiro",
    "orientation": "portrait",
    "owner": "gikundiro",
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://gikundiro.com",
          "linking": {
            "prefixes": [
              "gikundiro://",
              "https://gikundiro.com",
              "https://www.gikundiro.com"
            ],
            "config": {
              "screens": {
                "index": "",
                "match": "match/:id",
                "matchday": "matchday",
                "matches": "matches",
                "matchDetail": "matches/:id",
                "tickets": "tickets",
                "ticketDetail": "tickets/:id",
                "orders": "orders",
                "orderDetail": "orders/:id",
                "wallet": "wallet",
                "shop": "shop",
                "product": "shop/:slug",
                "members": "members",
                "support": "support",
                "settings": "more/settings",
                "more": "more",
                "qrGate": "ops/gate"
              }
            }
          }
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.rayonsports.fanapp",
      "associatedDomains": [
        "applinks:gikundiro.com",
        "applinks:www.gikundiro.com"
      ]
    },
    "android": {
      "package": "com.rayonsports.fanapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "gikundiro.com"
            },
            {
              "scheme": "https",
              "host": "www.gikundiro.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            {
              "scheme": "gikundiro"
            }
          ]
        }
      ]
    },
    "extra": {
      "router": {
        "origin": "https://gikundiro.com"
      }
    }
  }
}
